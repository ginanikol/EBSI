{
  "name": "@cef-ebsi/siop-auth",
  "version": "4.0.0-alpha.3",
  "type": "module",
  "description": "EBSI DID Auth library",
  "author": "",
  "license": "EUPL-1.2",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "bitbucket",
    "url": "https://ec.europa.eu/digital-building-blocks/code/scm/ebsi/core-libs.git",
    "directory": "packages/siop-auth"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "run-s clean build:*",
    "build:cjs": "tsc -p tsconfig/tsconfig.build.cjs.json && echo '{\"type\": \"commonjs\"}'> dist/cjs/package.json",
    "build:esm": "tsc -p tsconfig/tsconfig.build.esm.json",
    "clean": "rimraf dist coverage log build",
    "lint": "yarn lint:tsc",
    "lint:tsc": "tsc --noEmit --incremental false",
    "test": "vitest --run",
    "test:unit": "yarn test unit",
    "test:e2e": "yarn test e2e",
    "test:ci": "yarn test:unit --coverage --reporter=default --reporter=vitest-sonar-reporter"
  },
  "dependencies": {
    "@cef-ebsi/ebsi-did-resolver": "^4.0.0-alpha.2",
    "@cef-ebsi/key-did-resolver": "^2.0.0-alpha.2",
    "@cef-ebsi/key-encoder": "^1.0.0-alpha.2",
    "@noble/hashes": "^1.3.2",
    "axios": "^1.5.1",
    "did-jwt": "^7.3.0",
    "did-resolver": "^4.1.0",
    "eciesjs": "^0.4.5",
    "elliptic": "^6.5.4",
    "jose": "^4.14.6",
    "multiformats": "^12.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.15",
    "@types/uuid": "^9.0.5"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "gitHead": "a5798f045f97b434a16011edb8d2f9950906955f"
}
